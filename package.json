{
  "name": "vitepress-plugin-sidebar-depth",
  "type": "module",
  "version": "1.0.0",
  "description": "扫描 markdown 文档，自动生成侧边栏，自定义递归深度",
  "keywords": [
    "vitepress",
    "vite",
    "plugin",
    "sidebar"
  ],
  "license": "MIT",
  "homepage": "https://github.com/liyao52033/vitepress-plugin-sidebar-depth.git",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liyao52033/vitepress-plugin-sidebar-depth.git"
  },
  "bugs": {
    "url": "https://github.com/liyao52033/vitepress-plugin-sidebar-depth/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "stub": "unbuild --stub",
    "build": "unbuild && tsx src/replace-export.ts"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsx": "^4.19.4",
    "unbuild": "^3.2.0",
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  },
  "peerDependencies": {
    "vite": "^6.0.7",
    "vitepress": "^1.6.3"
  }
}
